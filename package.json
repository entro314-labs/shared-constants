{
  "name": "@entro314labs/shared-constants",
  "version": "1.0.0",
  "description": "Shared constants and configuration across the Entrolytics ecosystem",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "biome check .",
    "format": "biome format --write ."
  },
  "keywords": [
    "entrolytics",
    "constants",
    "config",
    "shared"
  ],
  "author": "Entro314 Labs",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
